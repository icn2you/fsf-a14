<div id="{{ _id }}" class="card">
  <img src="{{ image }}" class="card-img-top" alt="{{ title }}">
  <div class="card-body">
    <h5 class="card-title">{{ title }}</h5>
    <p class="card-text">This is only a test.</p>
    <p class="card-text"><small class="text-muted">{{ date }}</small></p>
  </div>
</div>
